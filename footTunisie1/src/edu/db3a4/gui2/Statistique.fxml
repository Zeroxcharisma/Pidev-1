<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.BarChart?>
<?import javafx.scene.chart.CategoryAxis?>
<?import javafx.scene.chart.NumberAxis?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="edu.db3a4.gui2.StatistiqueController">
   <children>
      <BarChart fx:id="TerrainChart" prefHeight="400.0" prefWidth="600.0" title="Terrains ">
        <xAxis>
          <CategoryAxis label="Terrain" side="BOTTOM" fx:id="x" />
        </xAxis>
        <yAxis>
          <NumberAxis fx:id="y" label="Nombre de tournoi" side="LEFT" />
        </yAxis>
      </BarChart>
   </children>
</AnchorPane>
